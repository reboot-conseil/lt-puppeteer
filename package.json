{
  "name": "lt-puppeteer-copilot",
  "version": "1.0.0",
  "description": "Some startup code to get used to `puppeteer` and e2e tests.",
  "scripts": {
    "dev": "npx serve .",
    "test": "concurrently \"npm run dev\" \"npm run test:puppeteer\"",
    "test:puppeteer": "jest --detectOpenHandles --forceExit --verbose --runInBand tests"
  },
