  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "puppeteer": "^21.4.0",
    "serve": "^14.2.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
